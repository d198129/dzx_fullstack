/* pages/search/search.wxss */
page {
  background-color: #dedede;
  padding: 20rpx;
}
.search_row {
  height: 60rpx;
  display: flex;
}
.search_row input {
  flex: 1;
  height: 100%;
  background-color: #fff;
}
.search_row button {
  width: 110rpx !important;
  height: 100%;
  text-align: center;
  font-size: 26rpx;
  padding: 10rpx 0;
  margin: 0 10rpx !important;
}
.search_content {
  margin-top: 30rpx;
}
.search_content .search_item {
  background-color: #fff;
  font-size: 26rpx;
  padding: 15rpx 10rpx;
  border-bottom: 1rpx solid #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
